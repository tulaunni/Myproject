    public SA_TicketTemplateController(){
        try{
            dummylist = new LIST<String>();
            TransactionSET= new SET<Id>();
            selectedEvent =Apexpages.currentPage().getparameters().get('id');
            //selectedEvent='a0OZ000000BXgHC';
            if(selectedEvent != NULL){
                dummylist = selectedEvent.split(',',0);
            }
            string eventval = selectedEvent.substring(0,15);
            system.debug('@@@@ '+eventval);
            system.debug('^^^^^ '+dummylist);
            CheckBoolean = false;
            RegInfoInnerList= new LIST<RegInfoInner>();
            listdum = new  LIST<Dummay__c>();
            
            listdum = [SELECT Event__r.id,Event__r.EV_EventLogo__c,Event__r.EV_Title__c, Payment_Type__c,Event__r.Sales_Tax__c,
                       Event__r.EV_StartDate__c,Event__r.StartTime__c,Event__r.EV_EndDate__c,First_Name__c,Last_Name__c,
                       Company__c,customer_key__r.Name,createdDate, Event__r.EV_EndTime__c,Event__r.EV_HostingTimeZone__c,
                       Event__r.EV_HostingLocation__c,Event__r.EV_HostingCountryNew__c,Amount__c,Available_Quantity__c,
                       Event__r.EV_HostingStateNew__c,Event__r.EV_HistingAddr1__c,Event__r.AS_HostingAddr2__c,Event__r.EV_HostingZipcode__c,
                       Event__r.AS_HostingCity__c,TicketTypes__r.TT_Name__c,Email__c,Payment_Status__c,Tickets_Quantty__c,
                       Item__r.Sales_Tax__c,Item__r.Item_name__c,Item_Quantity__c,TicketAmount__c, TicketTypes__r.TT_TicketType__c,
                       Item__r.Cost__c, TicketTypes__r.TT_Amount__c, Check_number__c, Credit_Card_Number__c, payKey__c, customer_key__c,
                       customer_key__r.Total_Amount__c, customer_key__r.ScanAttendee_Fee__c, customer_key__r.Amount_With_Fee__c, 
                       ScanAttendee_Fee__c, Total_Amount__c
                       FROM Dummay__c 
                       WHERE Id IN: dummylist];
            system.debug(listdum.size()+'  **** '+listdum);
            
            TotalAmount = 0.00;
            TotalAmountWithoutTax= 0.00;
            TaxAmount= 0.0;
            TotalScanAttendeeFee=0.0;
            ItemTaxValue= 0;
            String transactionId;
            if(listdum.size() >0){
                transactionId= listdum[0].customer_key__c;
                ConfirmationNumber= listdum[0].payKey__c;
                TotalAmount= listdum[0].customer_key__r.Amount_With_Fee__c;
                TotalScanAttendeeFee= listdum[0].customer_key__r.ScanAttendee_Fee__c;
                system.debug(TotalAmount+'  TOTAL AMOUNT IN IF '+TotalScanAttendeeFee);
                for(Dummay__c d:listdum){
                    
                    TransactionSET.add(d.customer_key__c);
                    Decimal withoutTaxAmount= 0.0;
                    PaymentType= d.Payment_Type__c;
                    
                    system.debug(d.TicketTypes__c+'  TICKET OR ITEM  '+d.Item__c);
                    
                    if(d.TicketTypes__c != NULL){
                        CheckBoolean= true;
                        if(PaymentType != 'Free'){
                            if(d.TicketTypes__r.TT_TicketType__c == 'Donation'){
                                withoutTaxAmount= d.Amount__c;                        
                            }else{
                                withoutTaxAmount= d.TicketTypes__r.TT_Amount__c * d.Available_Quantity__c;                        
                            }
                        }else{
                            withoutTaxAmount= 0;
                        }
                        TotalAmountWithoutTax += withoutTaxAmount;
                    }
                    
                    if(d.Item__c != NULL){
                        if(d.Item__r.Sales_Tax__c != 0){
                            ItemTaxValue= Integer.valueof(d.Event__r.Sales_Tax__c);
                            TaxAmount += ((d.Item__r.Cost__c*d.Item__r.Sales_Tax__c)/100)*d.Available_Quantity__c ; 
                                                                                                                   
                        }
                        withoutTaxAmount= d.Item__r.Cost__c * d.Available_Quantity__c;
                        TotalAmountWithoutTax += withoutTaxAmount;
                    }
                    system.debug(d.ScanAttendee_Fee__c+'  TOTAL AMOUNT '+withoutTaxAmount+'  '+ TaxAmount +'   '+TotalAmountWithoutTax );
                    TotalAmount= TotalAmount;
                    withoutTaxAmount= withoutTaxAmount.setScale(2);
                    withoutTaxAmount= withoutTaxAmount+ d.ScanAttendee_Fee__c;
                    TotalAmountWithoutTax= TotalAmountWithoutTax+ d.ScanAttendee_Fee__c;
                    RegInfoInnerList.add(new RegInfoInner(d, withoutTaxAmount));
                
                }//.format();
                TotalAmount= TotalAmount.setScale(2);
                TotalAmountWithoutTax= TotalAmountWithoutTax.setScale(2);
                TaxAmount= TaxAmount.setScale(2); 
                if(PaymentType == 'Check'){
                    CheckNumber= listdum[0].Check_number__c;                
                }else if(PaymentType == 'Credit Card'){
                    CheckNumber= '************'+listdum[0].Credit_Card_Number__c;                                    
                }
                
                Constomer__c ItemTransactions= new Constomer__c();
                ItemTransactions= [SELECT ID, (SELECT ID FROM Dummays__r WHERE TicketTypes__c !=: NULL) FROM Constomer__c WHERE Id =: transactionId];
                LIST<Dummay__c> TicketsList= new LIST<Dummay__c>();
                TicketsList= ItemTransactions.Dummays__r;
                ShowQrCode= FALSE;
                if(TicketsList.size() >0){
                    ShowQrCode= TRUE;
                }
            }
                       
            dum = new Dummay__c();
            dum = [SELECT Event__r.id,Event__r.EV_EventLogo__c,Event__r.EV_Title__c, Payment_Type__c,Event__r.EV_StartDate__c,Event__r.StartTime__c,Event__r.EV_EndDate__c,First_Name__c,Last_Name__c,Company__c,customer_key__r.Name,createdDate,
                          Event__r.EV_EndTime__c,Event__r.EV_HostingTimeZone__c,Event__r.EV_HostingLocation__c,Event__r.EV_HostingCountryNew__c,
                          Event__r.EV_HostingStateNew__c,Event__r.EV_HistingAddr1__c,Event__r.AS_HostingAddr2__c,Event__r.EV_HostingZipcode__c,Event__r.AS_HostingCity__c,TicketTypes__r.TT_Name__c,Email__c,Payment_Status__c,Tickets_Quantty__c,Item__r.Sales_Tax__c,Item__r.Item_name__c,Item_Quantity__c,TicketAmount__c
                   FROM Dummay__c 
                   WHERE Id=:eventval];
                   
            if(dum!=NULL){
                system.debug('     haiiiiiiiiiiiiiiiiiiiiiiiiii '+selectedEvent);
                AttendeeSummary__c attsum = new AttendeeSummary__c();
                attsum = [select id from AttendeeSummary__c where AS_Email__c=:dum.Email__c LIMIT 1];
                system.debug(dum.Event__c+' AAAAAAAAAAAAAAAAAAAAAA '+attsum);   
                
                eam =new Event_Attendee_Mapping__c();
                eam =[select id,Attendee_Event_IDs__c,EA_AttendeeBarcode__c,AS_id__r.AS_Email__c,EV_id__r.EV_Title__c,
                      AS_id__r.AS_FirstName__c,AS_id__r.AS_LastName__c,AS_id__r.AS_Company__c,EV_id__r.EV_StartDate__c,
                      EV_id__r.StartTime__c,EV_id__r.EV_EndDate__c,EV_id__r.EV_EndTime__c,EV_id__r.EV_HostingLocation__c,
                      EV_id__r.EV_HostingStateNew__c,EV_id__r.EV_EventLogo__c,EV_id__r.EV_HistingAddr1__c,
                      EV_id__r.AS_HostingAddr2__c,EV_id__r.EV_HostingZipcode__c,EV_id__r.AS_HostingCity__c, EV_id__r.Fee_Type__c 
                      from Event_Attendee_Mapping__c 
                      where (EV_id__c=:dum.Event__c) 
                      and  (AS_id__c =:attsum.id) limit 1];
                system.debug(' BBBBBBBBBBBBBBBBBBBBBB '+eam.EV_id__r.EV_Title__c );  
                isOneDayEvent= FALSE;   
                if(eam.EV_id__r.EV_StartDate__c == eam.EV_id__r.EV_EndDate__c){
                    isOneDayEvent= TRUE;
                }
                isCustomerPayFee= FALSE;

                if(eam.EV_id__r.Fee_Type__c == 'Charge the fee to the customer'){
                    isCustomerPayFee= TRUE;
                    system.debug('  CUSTOMER PAY FEE '+isCustomerPayFee);
                }
                system.debug(isCustomerPayFee+'  AFTER IF '+eam.EV_id__r.Fee_Type__c+'  PAYMENT TYPE '+PaymentType);
                if(eam.EV_id__r.EV_HostingStateNew__c!=null){
                    system.debug(' STSTE '+eam.EV_id__r.EV_HostingStateNew__c);
                    Commanforstates  con = new Commanforstates ();
                    eam.EV_id__r.EV_HostingStateNew__c=  con.mapstate(eam.EV_id__r.EV_HostingStateNew__c);
                    //EventState= con.mapstate(dum.Event__r.EV_HostingStateNew__c);
                    system.debug(' EVENT STATE '+eam.EV_id__r.EV_HostingStateNew__c);                    
                } 
            }  
            
            if(TransactionSET.size() >0){
                BarCodeForCustomer= new LIST<Constomer__c>();
                BarCodeForCustomer= [SELECT ID, Barcode__c FROM Constomer__c WHERE ID IN : TransactionSET];
            }       
        }
        catch(Exception ex){}
        fileformat = 'pdf';
    }
    
