.helpdiv {
		width: 200px;
		height: auto;
		background: #F50;
		margin: 10px;
		position: relative;
		
		border-radius: 20px;
		-webkit-border-radius:20px;
		-moz-border-radius:20px;
		
		
	}
	blockquote {
		margin: 0px;
		padding: 15px;
		color: #FFF;
		font: bold 14px/20px Arial;
	}
	.arrow {
		width:0px; 
		height:0px; 
		margin:0 auto;
		font-size:0px;
		line-height:0px;
		border-bottom:10px solid #F50;
		border-left:10px solid transparent;
		border-right:10px solid transparent;
		position: absolute;
		top: -10px;
		left: 90px;
	}





