<apex:page controller="BLN_ExhibitorScanReportCon" contentType="application/vnd.ms-excel#exportAllExhibitorScans.xls" cache="true" readOnly="true" action="{!exportallmethod}">
<table border="1">
<tr>
    <th>Exhibitor Name</th>
    <th>Company</th>
    <th>Email</th>
    <th>Attendee Name</th>
    <th>Title</th>
    <th>Company</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Scan Time</th>
</tr>
<apex:repeat value="{!allLeads}" var="al">
<tr>
    <td>{!al.Scanner_User__r.First_Name__c} {!al.Scanner_User__r.Last_Name__c}</td>
    <td>{!al.Scanner_Company__r.Name}</td>
    <td>{!al.Scanner_User__r.Email__c}</td>
    <td>{!al.First_Name__c} {!al.Last_Name__c}</td>
    <td>{!al.Title__c}</td>
    <td>{!al.Company__c}</td>
    <td>{!al.Cell_Phone__c}</td>
    <td>{!al.Lead_Email__c}</td>
    <td><c:EV_FormatDt date_Timevalue="{!al.Scan_Time__c}" t_Zone="{!al.Event__r.Time_Zone__c}"/></td>
</tr>
</apex:repeat>
</table>
</apex:page>