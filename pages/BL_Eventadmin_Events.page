<apex:outputPanel styleClass="popupBackground" layout="block" rendered="{!promosDisplaybool}"/>
         <apex:outputPanel styleClass="custPopup" layout="block" rendered="{!promosDisplaybool}" style="width:65%;">
          <table width="100%" border="0" align="center" cellpadding="0" cellspacing="0" >
          <tr>
            <td colspan="3" height="35px" bgcolor="#75b9fe" align="center" valign="middle" style=" color:#FFFFFF; padding-left:15px;" class="pophead"><h2><font size="4"  color="#FFFFFF">Add your Promo code</font></h2>
             <div style="float:right;"><apex:commandButton action="{!Cancel}" value="Close" title="Close" image="{!$Resource.Close1}" immediate="true" style="background-image:none;"/> </div>
            </td>
            </tr>
            </table>
            
            <table width="98%" border="0" align="center" cellpadding="0" cellspacing="0" class="tablesq" style="margin:0px auto;" >
            <tr>
                <td colspan="3" height="35px">
                    <apex:commandButton value="Add Promo Code" action="{!Promocodes}" styleClass="awesome" style="float:right;"/>
                </td>
            </tr>
            
            <tr>
              <th width="10%">Action</th>
              <th width="20%"> Promo Code</th>
              <th width="20%">Discount Type</th>
              <th width="20%">Discount Value</th>
           </tr>
           
           <apex:repeat value="{!TMPCODE}" var="code" >
          <tr>
          <td width="20%" style="border-right:0px;">
             <apex:commandLink action="{!editpromo}" title="Edit">
                          <apex:image url="{!$Resource.EditIconforQrcode}"/>
                          <apex:param value="{!code.id}" name="promoid"/>
                      </apex:commandLink>  |
                      
                      <apex:commandLink action="{!deletepromo}" onclick="return confirmdel();">
                    <apex:param value="{!code.id}" name="promoid"/>
                    <apex:image value="{!$Resource.Qrcode_delete}"/>
                    </apex:commandLink>
                      </td>           
            <td width="30%" style="border-right:0px;"><apex:outputText > {!code.Promocodes__r.Name}</apex:outputText></td>
            <td width="10%" style="border-right:0px;"><apex:outputText > {!code.Promocodes__r.Discount_Type__c}</apex:outputText></td>
            <td width="30%" style="border-right:0px;"><apex:outputText > {!code.Promocodes__r.Discount_Value__c}</apex:outputText></td>
          </tr>
    </apex:repeat>
    </table>
           
     
      
         </apex:outputPanel>
         </apex:form>
          <script type="text/javascript">
    function checkAll(cb,cbid)
    {
    var inputElem = document.getElementsByTagName("input");                    
    for(var i=0; i<inputElem.length; i++)
    {            
    if(inputElem[i].id.indexOf(cbid)!=-1)
    {                                       
    inputElem[i].checked = cb.checked;
    }
    }
    }
    function valid(){
    var tic = document.getElementById('checkedone');
    var ticname = tic.value();
    if(ticname == null || ticname == ''){
    
    alert('Please select atleast one ticket type to applay promo code');
    
    }
    }
    </script>
         <apex:form >
  <!---------------------------------------------------Promocodes--------------------->
  <apex:outputPanel styleClass="popupBackground" layout="block" rendered="{!promosbool}"/>
         <apex:outputPanel styleClass="custPopup" layout="block" rendered="{!promosbool}" style="width:65%;">
         <table width="100%" border="0" align="center" cellpadding="0" cellspacing="0" >
          <tr>
            <td colspan="3" height="35px" bgcolor="#75b9fe" align="center" valign="middle" style=" color:#FFFFFF; padding-left:15px;" class="pophead"><h2><font size="4"  color="#FFFFFF">{!pro}</font></h2>
             <div style="float:right;"><apex:commandButton action="{!CancelSchedule}" value="Close" title="Close" image="{!$Resource.Close1}" immediate="true" style="background-image:none;"/> </div>
            </td>
            </tr>
            </table>
            
        <table width="100%" border="0" align="center" cellpadding="0" cellspacing="0" style="line-height:35px;">
            <tr>
                 <td colspan="2">&nbsp;</td>
             </tr>
             <tr>
                 <td align="left" width="45%" valign="middle" style="padding-left:20px;"> <apex:outputText value="Discount Type"/></td>
                 <td align="left" width="55%" valign="middle"><apex:inputField value="{!Pcode.Discount_Type__c}" required="true" style="width:180px;"/></td>
             </tr>
             <tr>
                 <td align="left" valign="middle" style="padding-left:20px;"> <apex:outputText value="Discount Value"/></td>
                 <td><apex:inputField value="{!Pcode.Discount_Value__c}" required="true" style="width:180px;"/></td>
             </tr>
             <tr>
                 <td align="left" valign="middle" style="padding-left:20px;"> <apex:outputText value="Promo Code"/></td>
                 <td><apex:inputField value="{!Pcode.Name}" style="width:180px;" required="true"/></td>
             </tr>
             <tr>
                 <td align="left" valign="middle" style="padding-left:20px;"> <apex:outputText value="Expire Date"/></td>
                 <td><apex:inputField value="{!Pcode.Expiredate__c}" style="width:180px;" required="true"/></td>
             </tr>
             <tr>
                 <td align="left" valign="middle" style="padding-left:20px;"> <apex:outputText value="max number of uses allowed"/></td>
                 <td><apex:inputText value="{!noofattempts}" style="width:180px;"/></td>
             </tr>
             
             <tr> 
                 <td align="left" valign="middle" style="padding-left:20px;"> <apex:outputText value="Ticket Type"/></td>
                <td align="left" valign="middle">
                        <div style="width:350px;height:20px; background-color:#eaeaea;border:3px solid #CCCCCC;">
                 
                <apex:inputCheckbox onclick="checkAll(this,'checkedone');" id="all" />
                </div>
                       <div style="overflow: scroll;height:150px; width:350px; border:3px solid #CCCCCC; background-color:#eaeaea;">
                             <apex:selectCheckboxes value="{!selectedatttype}" layout="pageDirection" id="checkedone" onclick="selectAll(this,'all')">
                            
                             <apex:selectOptions value="{!tics}"/>
                             </apex:selectCheckboxes>
                           </div>
                </td>
            </tr>
             <tr>
                 <td align="left" valign="middle" style="padding-left:20px;">&nbsp;</td>
                 <td align="left" valign="middle" style="padding-left:20px;">&nbsp;</td>
             </tr>
             
              <tr>
              <!--
                  <td align="left" valign="middle" style="padding-left:20px;">&nbsp;</td>
                  <td align="left" valign="middle"><apex:commandButton value="save" action="{!savePromo}" styleClass="awesome"/> &nbsp;
                  <apex:commandButton value="save & New" action="{!saveandnewpromo}" styleClass="awesome"/>
                  </td>
                  -->
                  <apex:outputPanel rendered="{!foradd}" id="addsave">
                  <td align="left" valign="middle"><apex:commandButton value="save" action="{!savePromo}" styleClass="awesome"/> &nbsp;
                  <apex:commandButton value="save & New" action="{!saveandnewpromo}" styleClass="awesome"/>
                  </td>
                  </apex:outputPanel>
                  <apex:outputPanel rendered="{!forEdit}" id="editsave">
                  <td align="left" valign="middle"><apex:commandButton value="save" action="{!Updatepromo}" styleClass="awesome"/> &nbsp;
                  
                  </td>
          </apex:outputPanel>
              </tr>
          <tr>
             <td colspan="2">&nbsp;</td>
         </tr>
         
         </table>
         
         
         </apex:outputPanel>
        <apex:outputPanel rendered="{!errorpromobool}">
         <script>
         alert('This promocode is already Exist for this Event.Please enter other code');
 
         </script>
         </apex:outputPanel>
          <apex:outputPanel rendered="{!errorpromotics}">
         <script>
         alert('Please select atleast one ticket type to applay this promocode');
 
         </script>
         </apex:outputPanel>
<!--------------------------------------------------------------------------------------------------------->
</apex:form>
 
<!-- start footer -->        
<apex:include pageName="QRCode_Footertemplate"/>



</apex:page>