<apex:page controller="testgroupingCon"  showHeader="false"  renderAs="pdf" standardStylesheets="false" title="Registration_Details.pdf" contentType="application/pdf#Attendee_Registration_Details.pdf">
<apex:stylesheet value="{!URLFOR($Resource.pdfstyles, 'pdfstyles.css')}"/>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html;charset=UTF-8" />
</head>
<!--header-->
<div class="header" style="text-align:center;padding:0px; margin:0px;font-family: Arial,Helvetica,sans-serif;color:#04408c;">
<h3>{!selectedoption} Registration Details</h3>
<table width="100%" cellspacing="0" cellpadding="5" style="border:1px solid #CCCCCC;">
        <tr>
          <!--  <th class="tableHeader" style="width:14%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Registration Type</b></th> -->
            <th class="tableHeader" style="width:13%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Company</b></th>
            <th class="tableHeader" style="width:13%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Name</b></th>      
            <th class="tableHeader" style="width:13%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Email</b></th>
            <th class="tableHeader" style="width:10%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Phone</b></th>
            <th class="tableHeader" style="width:7%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Payment Type</b></th>
            <th class="tableHeader" style="width:7%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Payment Status</b></th> 
            <th class="tableHeader" style="width:7%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Registered Date</b></th>   
            <th class="tableHeader" style="width:15%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Ticket Type</b></th>          
            <th class="tableHeader" style="width:9%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Amount Paid</b></th>
            <th class="tableHeader" style="width:6%;font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;"><b>Discount</b></th>                 
        </tr>
        </table>
</div>  
<!-- header -->

<!--table-->
<body>
<div style="padding-top:10px;">

<table width="100%" cellspacing="0" cellpadding="5" border="0">
    <TBODY>  
        <apex:repeat var="a" value="{!dummylist1}">
            <tr style="border-top:1px solid #000000;page-break-after:auto;">
             <!-- <td class="tableContent" colspan="9" style="font-family: Arial,Helvetica,sans-serif;font-size:10px;color:#04408c; background-color:#eaeaea;"><b><apex:outputText value="{!a.dummyUsertype1}"/></b></td> -->               
              <td class="tableContent" colspan="10" style="font-family: Arial,Helvetica,sans-serif;font-size:10px;color:#04408c; background-color:#eaeaea;"><b><apex:outputText value="{!a.dummyCompany1}"/></b></td>
            </tr>  
        <apex:repeat var="c" value="{!a.liDummys1}">
            <tr style="page-break-after:auto;border-top:1px solid #CCCCCC;">
             <!-- <td style="text-align:left;width:16%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;"></td> -->
              <td style="text-align:left;width:13%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;"></td>
              <td class="tableContent" style="text-align:left;width:13%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;"><apex:outputText value="{!c.First_Name__c} {!c.Last_Name__c}"/></td>
              <td class="tableContent" style="text-align:left;width:13%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;"><apex:outputText value="{!c.Email__c}"/></td>
              <td class="tableContent" style="text-align:left;padding-left:7px;width:10%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;"><apex:outputText value="{!c.Phone__c}"/></td>
              <td class="tableContent" style="text-align:left;padding-left:7px;width:7%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;"><apex:outputText value="{!c.Payment_Type__c}"/></td>
              <td class="tableContent" style="text-align:left;padding-left:7px;width:7%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;"><apex:outputText value="{!c.Payment_Status__c}"/></td>
              <td class="tableContent" style="text-align:left;padding-left:7px;width:7%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;"><apex:outputText value="{0,date,MM/dd/yyyy hh:mm a}"><apex:param value="{!c.Registered_Date__c - 0.208333333}"/></apex:outputText></td>
              <td class="tableContent" style="text-align:left;padding-left:7px;width:15%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;"><apex:outputText value="{!c.customer_key__r.Ticket_Types__c}"/></td>              
              <td class="tableContent" style="text-align:left;padding-left:7px;width:9%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;text-align:right;"><apex:outputText value="{!c.IndividualAmount__c}"/></td>
              <td class="tableContent" style="text-align:left;padding-left:7px;width:6%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-bottom: 0px solid #CCCCCC;text-align:right;"><apex:outputText value="{!c.DiscountValue__c}"/></td>
           </tr>
        </apex:repeat>
        </apex:repeat> 
    </TBODY>
    <TFOOT>
      <tr>      
          <td colspan="10" style="font-family: Arial,Helvetica,sans-serif;font-size:9px;color:#04408c;background-color:#d6e3f2;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;text-align:right;">
          <div style="float:left;">
            Total Amount Through: &nbsp; Credit Card: &nbsp;&nbsp; {!TotalCCAmount} &nbsp;&nbsp;&nbsp;&nbsp; / &nbsp;
            Check: &nbsp; {!TotalCheckAmount} &nbsp;&nbsp;&nbsp;&nbsp; / &nbsp;
            Invoice: &nbsp; {!TotalInvoiceAmount} &nbsp;&nbsp;&nbsp;&nbsp;
          </div>
          <div style="float:right;">
            Total Amount: &nbsp; {!TotalAmount} &nbsp;
          </div>
          </td>
      </tr>
    </TFOOT>
    
</table> 
</div>
</body>

</html>
</apex:page>