   // temp=results;
  }
},  
    
getPersistedSelection: function() {
  return [].concat(this.selection);
}, 
   
clearPersistedSelection: function(){ 
    var changed = (this.selection.length > 0);   
    this.selection = [];
    this.selected = {};
    this.onViewRefresh();    
