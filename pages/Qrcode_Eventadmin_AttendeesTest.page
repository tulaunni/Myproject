    }  
   }   
  }  
},
   
onDeselectPage: function() {
 var store = this.grid.getStore(),
 selectedCheckboxLength= this.getPersistedSelection().length,
 i;
    
 for(i = store.getCount() - 1; i >= 0; i--) {
  this.onRowDeselect(this.selModel, store.getAt(i));
