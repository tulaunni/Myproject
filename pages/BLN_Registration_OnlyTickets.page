            {
                arr[i]=0;
            }
            //alert(arr);
            function fun(maxanswers,keyvalue,chk,ths){
               // alert(maxanswers+'==>'+keyvalue);       
                if(chk)
                {
                     arr[keyvalue]++;
                }
