               var x = 0;
     
              for (x=0;x<ele.length;x++)
                 {
                    if (ele[x].selected)
                    {
                       SelBranchVal1 += ele[x].value+'123543';
                    }
                  }
                
          }
             });      
          regsetlist = regsetlist+tabname+'1!@#'+SelBranchVal1+'-';     
   
       
      if(tabschkdval && tabname == ''){
               alert('Please Enter TabName');
               selbol = false;
               document.getElementById(aid).style.border = "1 px";
