<apex:page controller="MM_EventAdmin_ReportsCon"  showHeader="false"  renderAs="pdf" standardStylesheets="false" title="{!attendeealias}_Appointments.pdf" contentType="application/pdf#{!attendeealias}_Appointments.pdf">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html;charset=UTF-8" />
</head>
<!--header-->
<div class="header" style="text-align:center;padding:0px; margin:0px;font-family: Arial,Helvetica,sans-serif;color:#04408c;">

<table width="100%" border="0" align="center" cellpadding="0" cellspacing="0">

  <tr>
    <td width="19%" height="150" align="center" valign="middle" bgcolor="#f3f3f3">
        
        <!--<apex:image url="{!$Resource.NoEventLogo}" height="100px" width="100px" rendered="{!if(attach.EV_EventLogo__c== null,'TRUE','FALSE')}" style="margin:5px;border-radius:4px; -moz-border-radius:4px; -webkit-border-radius:4px;"/> --> 
        <apex:image url="/servlet/servlet.ImageServer?id={!attach.EV_EventLogo__c}&oid={!$Organization.Id}" height="100px" width="100px" rendered="{!if(attach.EV_EventLogo__c!= null,'TRUE','FALSE')}" style="margin:5px;border-radius:4px; -moz-border-radius:4px; -webkit-border-radius:4px;"/>

    </td>
    <td width="81%" align="left" valign="middle" bgcolor="#f3f3f3">
    <table width="100%" border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td width="50%">&nbsp;</td>
        </tr>
      <tr>
        <td align="center" valign="middle"><h1 style="color:#0f71b5;font-family:Verdana, Geneva, sans-serif; font-size:16px;">{!attach.EV_Title__c}</h1></td>
        </tr>
      <tr>
        <td align="center" valign="middle" style="font-family:Verdana, Geneva, sans-serif; font-size:12px;padding:5px;">
        <apex:outputText value="{0,date, MMM' 'dd', 'yyyy}" rendered="{!IF(MONTH(EventDetails.EV_StartDate__c)!=6,true,false)}">
   <apex:param value="{!EventDetails.EV_StartDate__c}" /> 
   </apex:outputText>


   <apex:outputText value="{0,date, MMMM' 'dd', 'yyyy}" rendered="{!IF(MONTH(EventDetails.EV_StartDate__c)==6,true,false)}">
   <apex:param value="{!EventDetails.EV_StartDate__c}" /> 
   </apex:outputText>

        </td>
        </tr>
      <tr>
        <td align="center" valign="middle" style="font-family:Verdana, Geneva, sans-serif; font-size:12px;padding:5px;">{!attach.EV_HostingLocation__c}</td>
      </tr>
      <tr>
        <td align="center" valign="middle" style="font-family:Verdana, Geneva, sans-serif; font-size:12px;padding:5px;">{!attach.EV_HistingAddr1__c}</td>
      </tr>
      <tr>
        <td align="center" valign="middle" style="font-family:Verdana, Geneva, sans-serif; font-size:12px;padding:5px;">{!attach.AS_HostingCity__c}<apex:outputText value="," rendered="{!attach.AS_HostingCity__c!=Null}"/> {!attach.EV_HostingStateNew__c} {!attach.EV_HostingZipcode__c}</td>
        </tr>
    </table>
    </td>
  </tr>
</table></div>
<br/>

<div>
  
<table width="100%" cellspacing="0" cellpadding="5">
     <tr>         
            <th class="tableHeader" style="width:15%;font-family: Arial,Helvetica,sans-serif;font-size:11px;background-color:#F3F3F3;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;text-align:left;"><b>{!attendeealias} </b></th>             
            <th class="tableHeader" style="width:15%;font-family: Arial,Helvetica,sans-serif;font-size:11px;background-color:#F3F3F3;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;text-align:left;"><b>{!attendeealias} Name</b></th>           
            <th class="tableHeader" style="width:15%;font-family: Arial,Helvetica,sans-serif;font-size:11px;background-color:#F3F3F3;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;text-align:left;"><b>Email</b></th>           
            <th class="tableHeader" style="width:15%;font-family: Arial,Helvetica,sans-serif;font-size:11px;background-color:#F3F3F3;border-bottom: 1px solid #CCCCCC;border-left: 1px solid #CCCCCC;border-top: 1px solid #CCCCCC;text-align:left;"><b>Number of Appointments</b></th>
            
        </tr> 
    <apex:repeat var="print" value="{!eamcountlist2}">  
    <tr style="page-break-after:auto;border-top:0px solid #CCCCCC;"> 
              <td style="text-align:left;padding-left:7px;width:15%;font-family: Arial,Helvetica,sans-serif;font-size:10px;border-bottom: 0px solid #CCCCCC;border-right: 0px solid #CCCCCC;"><apex:outputText value="{!print.inneam.AS_id__r.AS_Company__c}"/></td>   
              <td style="text-align:left;padding-left:7px;width:15%;font-family: Arial,Helvetica,sans-serif;font-size:10px;border-bottom: 0px solid #CCCCCC;border-right: 0px solid #CCCCCC;"><apex:outputText value="{!print.inneam.AS_id__r.FullName__c}"/></td>                
              <td style="text-align:left;padding-left:7px;width:15%;font-family: Arial,Helvetica,sans-serif;font-size:10px;border-bottom: 0px solid #CCCCCC;border-right: 0px solid #CCCCCC;"><apex:outputText value="{!print.inneam.AS_id__r.AS_Email__c}"/></td>
              <td style="text-align:left;padding-left:7px;width:15%;font-family: Arial,Helvetica,sans-serif;font-size:10px;border-bottom: 0px solid #CCCCCC;border-right: 0px solid #CCCCCC;"><apex:outputText value="{!print.i}"/></td>
              
           </tr>
        </apex:repeat>
    </table> 
</div>

</html>
</apex:page>