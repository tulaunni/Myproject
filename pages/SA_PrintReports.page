              <td class="tableContent" style="text-align:left;width:6%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-top: 1px black;"><apex:outputText value="{!c.TicketTypes__r.TT_TicketType__c}"/></td>
              <td class="tableContent" style="text-align:left;width:9%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-top: 1px black;"><apex:outputText value="{!c.customer_key__r.Name}"/></td>
              <td class="tableContent" style="text-align:left;width:9%;font-family: Arial,Helvetica,sans-serif;font-size:9px;border-top: 1px black;"><apex:outputText value="{0,date,MM/dd/yyyy hh:mm a}"><apex:param value="{!c.Registered_Date__c}"/></apex:outputText></td>
