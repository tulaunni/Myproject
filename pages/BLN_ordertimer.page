<apex:page controller="BLN_RegistrationnewCon3" showHeader="false" standardStylesheets="false" id="BLN_ordertimer">
  
  <div class="col-lg-12" style="background-color:#FFF;">
    <!-- count down timer start -->
    <div class="row" style="padding:5px;"> 
      <!------->
      <div class="col-lg-12 col-sm-12 col-md-12 col-xs-12" style="background-color:#ffffce; padding:5px; border:1px solid #F3F3AF; border-radius:3px;">
        <div class="col-lg-2 col-sm-4 col-md-4 col-xs-4   text-right">
          <h4 class="text-center"><span id="minutes" class="numbers"></span> Minutes</h4>
        </div>
        <div class="col-lg-1 col-sm-4 col-md-4 col-xs-4 text-center">
          <h4>:</h4>
        </div>
        <div class="col-lg-2 col-sm-4 col-md-4 col-xs-4 text-left">
          <h4 class="text-center"><span id="seconds" class="numbers"></span> Seconds</h4>
        </div>
        <div class="col-lg-7 col-sm-12 col-md-12 col-xs-12">
          <p> Please complete registration within <span id="time_limit">{!SessionTime}</span> minutes. After <span id="time_limit2">{!SessionTime}</span> minutes, the reservation we are holding will be released to others. </p>
        </div>
        <!-------> 
      </div>
    </div>
    </div>
  
</apex:page>